{
    "name": "plainphp/package-installer",
    "type": "composer-plugin",
    "require": {
        "composer-plugin-api": "1 - 2"
    },
    "extra": {
        "class": "PlainPhp\\Composer\\PackageInstallerPlugin",
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    },
    "autoload":{
        "psr-4":{
            "PlainPhp\\Composer\\":"/src/"
        }
    }
}